{
  "name": "my-qwik-docs-starter",
  "description": "Documentation site starter",
  "engines": {
    "node": ">=15.0.0"
  },
  "license": "",
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src/**/*.ts*",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "serve": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.14.0",
    "@builder.io/qwik-city": "0.0.124",
    "@emotion/react": "11.9.3",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.9.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/eslint": "8.4.6",
    "@types/node": "^18.11.9",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "autoprefixer": "^10.4.12",
    "eslint": "8.23.1",
    "eslint-plugin-qwik": "0.10.0",
    "node-fetch": "3.2.10",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.3",
    "vite": "3.1.1",
    "vite-tsconfig-paths": "3.5.0"
  },
  "dependencies": {
    "@emailjs/browser": "^3.7.0",
    "express": "^4.18.2",
    "gray-matter": "^4.0.3"
  }
}
